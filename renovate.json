{
  "extends": [
    "config:base"
  ],
  "major": {
    "enabled": false
  },
  "prCreation": "not-pending",
  "prConcurrentLimit": 2,
  "packageRules": [
    {
      "packageNames": [
        "@rails/webpacker",
        "babel",
        "bundler",
        "node-sass",
        "rails",
        "react",
        "react-dom",
        "react-select",
        "yarn",
        "typescript"
      ],
      "packagePatterns": [
        "^(@babel|babel|webpacker|webpack|node)"
      ],
      "enabled": false
    }
  ],
  "hostRules": [
    {
      "hostType": "npm",
      "matchHost": "npm.powerapp.cloud",
      "authType": "Basic",
      "encrypted": {
        "token": ""
      }
    }
  ],
  "npmrc": "registry=https://npm.powerapp.cloud\n//npm.powerapp.cloud/:always-auth=true"
}
