<%= pb_form_with(method: :get, multipart: true) do |form| %>
  <%= form.text_field :name, style: "width: 140px", props: { placeholder: "Name" } %>
  <%= pb_rails("file_upload", props: {id: "import_file ", input_options: { name: "import_file" }}) %>
  <%= pb_rails("button", props: { text: "Submit" } ) %>
<% end %>
