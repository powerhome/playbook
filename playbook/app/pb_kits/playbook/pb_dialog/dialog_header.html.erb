<%= content_tag(:div,
    id: object.id,
    data: object.data,
    aria: object.aria) do %>
        <%= pb_rails("flex", props: {classname:object.classname, spacing:"between", padding:"sm"}) do %>
            <%= object.title %>
            <form method="dialog"><button class="dialog-button-class"><%= pb_rails("icon", props:{icon: "times"}) %></button></form>
        <% end %>
        <%= pb_rails("section_separator") %>
<% end %>