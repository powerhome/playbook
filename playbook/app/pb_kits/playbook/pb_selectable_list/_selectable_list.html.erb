<%= content_tag(:div,
    aria: object.aria,
    class: object.classname,
    data: object.data,
    id: object.id) do %>
      <%= pb_rails("list") do %>
        <% object.items.each do |item| %>
          <%= pb_rails("selectable_list/selectable_list_item", props: item.merge(variant: object.variant) )%>
        <% end %>
      <% end %>
<% end %>
