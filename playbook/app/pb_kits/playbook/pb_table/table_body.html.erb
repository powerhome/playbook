<% if object.tag == "table" %>
    <% if object.draggable_container %>
      <%= pb_rails("draggable/draggable_container", props: { tag: "tbody", classname: object.classname, data: object.data }) do %>
        <%= content.presence %>
      <% end %>
    <% else %>
      <%= pb_content_tag(:tbody) do %>
        <%= content.presence %>
      <% end %>
    <% end %>
<% else %>
    <%= pb_content_tag do %>
      <%= content.presence %>
    <% end %>
<% end %>
