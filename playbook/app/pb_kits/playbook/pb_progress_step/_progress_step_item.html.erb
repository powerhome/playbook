<%= content_tag(:li,
  id: object.id,
  data: object.data,
  class: object.classname) do %>
    <div class="box">
      <div class="circle">
        <%= pb_rails("icon", props: { icon: object.icon, size: "xs" }) if object.icon.present? %>
      </div>
      <div class="content">
        <%= capture(&object.children) %>
      <div>
    </div>
<% end %>
