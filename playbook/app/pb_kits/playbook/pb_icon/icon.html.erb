<% if object.custom_icon %>
  <%= object.render_svg(object.custom_icon) %>
<% else %>
  <%= content_tag(:i, nil,
      id: object.id,
      data: object.data,
      class: object.classname
  ) %>
  <%= content_tag(:span, nil,
      aria: { label: "#{object.icon} icon" }.merge(object.aria),
      hidden: true
  ) %>
<% end %>
