<%= pb_rails("title", props: { size: 4, text: "Which of the following vehicles do you own?" }) %>

<br>

<%= pb_rails("caption", props: { text: "Car" }) %>
<%= pb_rails("toggle", props: {
  size: "sm",
  checked: false,
  name: "vehicle",
  value: "car"
}) %>

<br>

<%= pb_rails("caption", props: { text: "Bike" }) %>
<%= pb_rails("toggle", props: {
  size: "sm",
  checked: false,
  name: "vehicle",
  value: "bike"
}) %>
