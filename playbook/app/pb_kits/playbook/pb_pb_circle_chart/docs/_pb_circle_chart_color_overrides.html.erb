<% data = [
        {
          name: "Waiting for Calls",
          y: 41,
        },
        {
          name: "On Call",
          y: 49,
        },
        {
          name: "After Call",
          y: 10,
        },
      ] %>

<% chart_options = {
    series: [{ data: data }],
    plotOptions: {
      pie: {
        colors: ["#144075", "#1CA05C", "#F9BB00"],
      },
    },
  } %>

<%= pb_rails("pb_circle_chart", props: { options: chart_options }) %>