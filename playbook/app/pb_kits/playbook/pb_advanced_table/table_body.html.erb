<%= content_tag(:div,
      aria: object.aria,
      class: object.classname,
      data: object.data,
      id: object.id) do %>
        <% object.table_data.each do |row| %>
          <%= render_row_and_children(row, object.column_definitions) %>
        <% end %>
        <%= turbo_frame_tag 'godinho' do %>
          <%= link_to "Click here for Turbo!", full_list_path %>
        <% end %>
<% end %>