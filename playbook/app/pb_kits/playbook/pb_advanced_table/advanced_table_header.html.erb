<%= content_tag(:div,
      aria: object.aria,
      class: object.classname,
      data: object.data,
      id: object.id) do %>
        <%= pb_rails("table/table_row", props: {tag:"div"}) do %>
            <% object.column_definitions.each do |item| %>
                <%= pb_rails("table/table_header", props: { tag:"div", id:item[:accessor], classname:"table-header-cells"}) do %>
                    <%= item[:label] %>
                <% end %>
            <% end %>
        <% end %>
<% end %>