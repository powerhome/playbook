{
  "name": "playbook-project",
  "private": true,
  "scripts": {
    "watch-all": "yarn workspaces foreach -ivp run watch",
    "release-all": "yarn workspaces foreach -iv run release",
    "start-dev": "yarn nf start",
    "prepare": "husky install"
  },
  "workspaces": {
    "packages": [
      "playbook",
      "playbook-website"
    ]
  },
  "devDependencies": {
    "foreman": "3.0.1",
    "husky": "6.0.0",
    "lint-staged": "10.5.4"
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint",
    "*.rb": "bundle exec --gemfile=playbook-website/Gemfile rubocop --parallel"
  },
  "dependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-flow": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/traverse": "^7.22.5",
    "babel-eslint": "^10.1.0",
    "fs": "^0.0.1-security"
  }
}
