<%= content_tag(:div,
    id: object.id,
    data: object.data,
    class: object.classname) do %>
      <% if object.icon == true %>
        <%= pb_rails("icon", props: { icon: "clock", fixed_width: true }) %>
      <% end %>
      <%= content_tag(:span) do %>
        <%= object.format_start_time_string %>
      <% end %>
      --
      <%= content_tag(:span) do %>
        <%= object.format_end_time_string %>
        <%= content_tag(:span, class: "pb_end_time_timezone") do %>
          <%= object.pb_date_end_time.to_timezone.upcase %>
        <% end %>
      <% end %>
<% end %>