<%= content_tag(:div,
    class: "pb_list#{object.xpadding}#{object.borderless}#{object.dark}#{object.lg}#{object.layout}") do %>
    <%= content_tag(:"#{object.ordered}",
      id: object.id,
      data: object.data,
      class: object.classname) do %>
      <%= object.yield(context: self) %>
  <% end %>
<% end %>
