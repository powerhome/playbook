<%= content_tag(:div,
    id: object.id,
    data: object.data,
    class: object.classname("pb_person_contact_kit")) do %>
    <%= object.name %>
    <% object.contacts.each do |contact| %>
      <%= pb_rails("contact", props: {
        contact_type: contact[:contact_type],
        contact_value: contact[:contact_value],
      }) %>
    <% end %>
<% end %>
