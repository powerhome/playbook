<%= content_tag(:div,
    id: object.id,
    data: object.data,
    class: object.classname) do %>
    <%= pb_rails("caption", props: {dark: object.dark, text: object.day_of_week}) %>
    <%= pb_rails("title", props: {dark: object.dark, size: 4, tag: "span", text: object.formatted_month_and_day}) %>
<% end %>
