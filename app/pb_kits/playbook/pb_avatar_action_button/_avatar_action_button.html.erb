<%= content_tag(:div,
    id: object.id,
    data: object.data,
    class: object.classname) do %>
      
      <%= pb_rails("avatar", props: {
        name: object.name,
        size: object.size,
        image_url: object.image_url,
      }) %>
      <div class="icon-circle">
        <%= pb_rails("icon", props: { 
          icon: object.icon
        }) %>
      </div>
<% end %>
