<%= content_tag(:div,
    id: object.id,
    data: object.data(initials: object.initials),
    class: object.classname(object.kit_class),
    aria: object.aria) do %>
  <%= content_tag(:div,
      data: { initials: object.initials },
      class: "avatar_wrapper") do %>
    <%= object.image %>
  <% end %>
  <%= object.status %>
<% end %>
