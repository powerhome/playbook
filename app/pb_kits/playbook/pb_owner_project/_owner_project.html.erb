<%= content_tag(:div,
                id: object.id,
                data: object.data,
                class: object.classname("pb_owner_project")) do %>

  <%= pb_rails("caption", props: { text: "Project" })%>

  <%= object.title %>

  <%= pb_rails("body", props:  { color: "light" }) do %>
    <%= pb_rails("icon", props: { icon: "home" })%>
    <%= object.project_number %>
  <% end %>
<% end %>
