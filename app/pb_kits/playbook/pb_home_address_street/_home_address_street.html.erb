<%= content_tag(:div,
    id: object.id,
    data: object.data,
    class: object.classname) do %>

  <% if object.emphasis == "2" %>
    <%= pb_rails "body", props: {
      classname: "pb_home_address_street_address",
      color: "light",
      text: object.address_house_style,
      dark: object.dark
    } %>

    <%= pb_rails "body", props: {
      classname: "pb_home_address_street_address",
      color: "light",
      text: object.address_house_style2,
      dark: object.dark
    } %>

    <%= pb_rails "title", props: {
      size: 4,
      text: object.city_state_zip,
      dark: object.dark
    } %>
  <% else %>
    <%= pb_rails "title", props: {
      classname: "pb_home_address_street_address",
      size: 4,
      text: object.address_house_style,
      dark: object.dark
    } %>

    <%= pb_rails "title", props: {
      classname: "pb_home_address_street_address",
      size: 4,
      text: object.address_house_style2,
      dark: object.dark
    } %>

    <%= pb_rails "body", props: {
      color: "light",
      text: object.city_state_zip,
      dark: object.dark
    } %>
  <% end %>

  <% if object.home_id %>
    <%= pb_rails("hashtag", props: {
      text: "#{object.home_id}",
      url: object.home_url || "#",
      type: "home",
      dark: object.dark,
      classname: "home-hashtag"}) %>
  <% end %>

  <%= pb_rails "body", props: { color: "light", tag: "span", dark: object.dark } do %>
    <small><%= object.territory %></small>
  <% end %>

<% end %>
